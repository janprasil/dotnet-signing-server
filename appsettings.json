{
  "UseLocalDb": true,
  "Default": {
    "ConnectionString": "Host=localhost;Port=5432;Database=mydb;Username=myuser;Password=mypassword"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "Loki": {
    "Url": "loki-server-url",
    "Username": "",
    "Password": "",
    "BearerToken": "",
    "App": "dotnet-signing-server",
    "Environment": "development"
  },
  "AI": {
    "Enabled": false,
    "Provider": "google",
    "Google": {
      "ApiKey": "",
      "Model": "gemini-1.5-flash",
      "Endpoint": "https://generativelanguage.googleapis.com/v1beta"
    }
  },
  "Limits": {
    "PdfMaxBytes": 20971520,
    "ImageMaxBytes": 1048576,
    "AttachmentMaxBytes": 10485760,
    "RequestBodyLimitBytes": 41943040,
    "MaxConcurrentRequestsPerKey": 25
  },
  "AllowedHosts": "*",
  "TimestampAuthority": {
    "Url": "https://freetsa.org/tsr",
    "Username": "",
    "Password": ""
  },
  "Stripe": {
    "ApiKey": "api-key",
    "WebhookSecret": "whsec-key",
    "Currency": "EUR",
    "EnableTaxIdCollection": true,
    "RequireBillingAddress": true,
    "EnableAutomaticTax": true
  },
  "Smtp": {
    "Host": "localhost",
    "Port": 1025,
    "UseSsl": false,
    "FromAddress": "no-reply@example.com",
    "FromName": "DotNet Signing Server",
    "Username": "",
    "Password": ""
  },
  "Token": {
    "Secret": "secret"
  }
