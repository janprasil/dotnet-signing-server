@{
    var title = ViewData["Title"] as string ?? "DotNet Signing Server";
    var isAuthenticated = User?.Identity?.IsAuthenticated == true;
    var displayName = isAuthenticated ? User?.Identity?.Name : null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@title</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 0;
            background: #f6f7fb;
            color: #1f2933;
            display: flex;
            flex-direction: column;
            min-height: 100vh;
        }

        header {
            background: #1f2933;
            color: #fff;
            padding: 1rem 1.5rem;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        nav a {
            color: #fff;
            margin-right: 1rem;
            text-decoration: none;
            font-weight: 600;
        }

        nav a:last-child {
            margin-right: 0;
        }

        .main-wrapper {
            background: #fff;
            padding: 1.5rem;
            border-radius: 8px;
            box-shadow: 0 6px 18px rgba(0, 0, 0, 0.06);
        }

        h1,
        h2,
        h3 {
            margin-top: 0;
        }

        table {
            width: 100%;
            border-collapse: collapse;
            margin-top: 1rem;
        }

        th,
        td {
            padding: 0.6rem;
            border-bottom: 1px solid #e5e7eb;
            text-align: left;
        }

        th {
            background: #f3f4f6;
            font-weight: 600;
        }

        form {
            margin: 1rem 0;
        }

        label {
            display: block;
            font-weight: 600;
            margin-top: 0.5rem;
        }

        input,
        select {
            width: calc(100% - 1rem);
            padding: 0.5rem;
            margin-top: 0.25rem;
            border: 1px solid #cbd5e1;
            border-radius: 4px;
        }

        button,
        .linkButton {
            background: #2563eb;
            color: #fff;
            border: none;
            padding: 0.6rem 1.2rem;
            border-radius: 4px;
            cursor: pointer;

            text-decoration: none;
        }

        button:hover,
        .linkButton:hover {
            background: #1d4ed8;
        }

        main {
            flex: 1;
            width: calc(100% - 2rem);
            max-width: 960px;
            margin: 2rem auto;
        }

        .muted {
            color: #6b7280;
            font-size: 0.9rem;
        }

        .top-actions {
            display: flex;
            justify-content: flex-end;
            margin-top: 0.5rem;
        }

        .top-actions a {
            background: #2563eb;
            color: #fff;
            padding: 0.5rem 0.9rem;
            border-radius: 4px;
            text-decoration: none;
            margin-left: 0.5rem;
        }

        .flash {
            padding: 0.75rem 1rem;
            border-radius: 6px;
            margin-bottom: 1rem;
        }

        .flash.info {
            background: #e0f2fe;
            color: #075985;
        }

        .flash.error {
            background: #fee2e2;
            color: #991b1b;
        }

        footer {
            background: #111827;
            color: #e5e7eb;
            padding: 1.25rem 1.5rem;
            margin-top: 2rem;
        }

        .footer-content {
            max-width: 960px;
            margin: 0 auto;
            display: flex;
            gap: 1.5rem;
            align-items: flex-start;
            justify-content: space-between;
        }

        .footer-links {
            display: flex;
            flex-direction: column;
            gap: 0.35rem;
            font-size: 0.95rem;
            text-align: right;
        }

        .footer-links a {
            color: #cbd5e1;
            text-decoration: none;
        }

        .footer-links a:hover {
            color: #fff;
            text-decoration: underline;
        }

        .footer-links.two-cols {
            display: grid;
            grid-template-columns: repeat(2, auto);
            gap: 0.3rem 2rem;
        }

        .footer-links.two-cols a {
            display: block;
            color: #cbd5e1;
            text-decoration: none;
            font-size: 0.95rem;
        }

        .footer-links.two-cols a:hover {
            color: white;
        }
    </style>
</head>

<body>
    <header>
        <a href="/" style="text-decoration:none;color:white;">
            <div style="
    user-select: none;
    display: flex;
    gap: 0.5rem;
    width: 200px;
"><img src="data:image/png;base64,"
                    style="
    /* object-fit: cover; */
    flex-basis: 33%;
    flex: 1 1 auto;
" width="24">
                <div style="
    flex-basis: 66%;
    flex: 1 1 auto;
"><strong style="
    font-family: system-ui;
">Sign</strong>Proxy
                    <small style="
    display: block;
    font-family: system-ui;
"><span style="font-weight: lighter;font-family: system-ui;">by</span> Performance4 s.r.o.</small>
                </div>
            </div>
        </a>
        <nav>
            <a href="/">Home</a>
            @if (isAuthenticated)
            {
                <a href="/Account">Account</a>
                <a href="/ApiTokens">API Tokens</a>
                <a href="/Billing">Billing</a>
                <a href="/templates">PDF Templates</a>
                @if (!string.IsNullOrWhiteSpace(displayName))
                {
                    <span style="color:#cbd5e1; margin-left:1rem;">@displayName</span>
                }
                <form method="post" action="/Account/SignOut" style="display:inline-block; margin-left:0.5rem;">
                    @Html.AntiForgeryToken()
                    <button type="submit">Sign out</button>
                </form>
            }
            else
            {
                <a href="/pricing">Pricing</a>
                <a href="/Account/SignIn">Sign in</a>
                <a href="/Account/SignUp">Sign up</a>
            }
        </nav>
    </header>
    <main>
        <div class="main-wrapper">
            @RenderBody()
        </div>
    </main>
    <footer>
        <div class="footer-content">
            <div style="font-size:0.95rem; line-height:1.6;font-family: system-ui;">
                Created by <a style="font-weight:600;color:#cbd5e1; text-decoration:none;"
                    href="https://performance4.cz">Performance4 s.r.o.</a>
                <a href="/contact" style="color:#cbd5e1; text-decoration:none;margin-top:1rem;display:block;">Contact
                    information</a>
                <a href="mailto:support@performance4.cz"
                    style="color:#cbd5e1; text-decoration:none;display:block;">Email: support@performance4.cz</a>
            </div>
            <div class="footer-links two-cols">
                <a href="/Legal/TermsOfService">Terms of Service</a>
                <a href="/Legal/PrivacyPolicy">Privacy Policy</a>
                <a href="/Legal/DataProcessingAgreement">DPA</a>
                <a href="/Legal/ServiceLevelAgreement">SLA</a>

                <a href="/Legal/RefundPolicy">Refund Policy</a>
                <a href="/Legal/CookiesPolicy">Cookies Policy</a>
                <a href="/Legal/OpenSourceNotices">Open-Source Notices</a>
                <a href="/Legal/License">AGPL License</a>
            </div>
        </div>
    </footer>
    @RenderSection("Scripts", required: false)
</body></html>
